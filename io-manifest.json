{
    "application": {
      "appId": "maven",
      "assetId": "maven",
      "appType": "Webapp",
      "release": "<<RELEASE_TYPE>>",
      "projectLanguage": "Java",
      "platformVersion": "Java 11",
      "buildSystem": "Maven",
      "riskMatrixRevision": "default",
      "sourceFilePattern": "(?s).*((/[a-z]+)*)/src/main/java(?s).*",
      "sensitivePackagePattern": "(?s).*((/[a-z]+)*)/com/service/([A-Za-z]*).java(?s).*",
      "fileChangeThreshold": 20
    },
    "environment": {
      "enableJira": true,
      "scm": "github",
      "scmOwner": "Synopsys-SIG-RnD",
      "scmRepositoryName": "ShoppingCart",
      "scmBranchName": "master",
      "bitbucketUserName": null,
      "bitbucketPassword": null,
      "bitbucketApiUrl": "https://api.bitbucket.org/2.0/repositories/",
      "githubApiUrl": "https://api.github.com/repos/",
      "githubUsername": "sig-rnd-io-testuser",
      "githubToken": "<<GITHUB_ACCESS_TOKEN>>",
      "jiraProjectName": "<<JIRA_PROJECT_NAME>>",
      "jiraIssuesQuery": "<<JIRA_ISSUES_QUERY>>",
      "jiraApiUrl": "<<JIRA_API_URL>>",
      "jiraUserName": "<<JIRA_USERNAME>>",
      "jiraAuthToken": "<<JIRA_AUTH_TOKEN>>"
    },
    "security": {
      "activities": {
        "sast": {
          "rescanThreshold": 10
        },
        "sca": {
          "rescanThreshold": 10
        }
      }
    },
    "connectors": [
      
      {
        "connector_name": "jira",
        "fields": {
          "issuetype": "Bug",
          "projectkey": "<<JIRA_PROJECT_NAME>>",
          "assignee": "<<JIRA_ASSIGNEE>>",
          "url": "<<JIRA_API_URL>>",
          "username": "<<JIRA_USERNAME>>",
          "authtoken": "<<JIRA_AUTH_TOKEN>>"
        }
      },
      {
        "connector_name": "github",
        "fields": {
          "file_path": "workflowengine-results.sarif.json",
          "owner_name": "<<GITHUB_OWNER_NAME>>",
          "repository_name": "<<GITHUB_REPO_NAME>>",
          "ref": "<<GITHUB_REF>>",
          "commit_id": "<<GITHUB_COMMIT_ID>>",
          "username": "<<GITHUB_USERNAME>>",
          "accesstoken": "<<GITHUB_ACCESS_TOKEN>>"
        }
      }
    ],
    "buildbreaker": [
      {
        "activityname": "sast",
        "status": [
          "new",
          "open"
        ],
        "overallseverity": [
          "high"
        ],
        "categories": [
          {
            "name": "cross-site scripting",
            "severity": "high"
          },
          {
            "name": "sql injection",
            "severity": "high"
          }
        ]
      },
      {
        "activityname": "sca",
        "status": [
          "new"
        ],
        "overallseverity": [
          "high"
        ]
      }
    ],
    "tool_information": [
      {
        "tool_name": "polaris",
        "enabled": true,
        "fields": {
          "project_name": "<<POLARIS_PROJECT_NAME>>",
          "instance_url": "<<POLARIS_SERVER_URL>>",
          "auth_token": "<<POLARIS_ACCESS_TOKEN>>"
        }
      },
      {
        "tool_name": "blackduck",
        "enabled": "<<IS_SCA_ENABLED>>",
        "fields": {
          "project_name": "<<BLACKDUCK_PROJECT_NAME>>",
          "instance_url": "<<BLACKDUCK_SERVER_URL>>",
          "auth_token": "<<BLACKDUCK_ACCESS_TOKEN>>"
        }
      }
    ]
  }